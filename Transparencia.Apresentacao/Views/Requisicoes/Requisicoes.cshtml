@model List<Transparencia.Domain.Entidades.Requisicao>


@{
	ViewData["Title"] = "Requisicoes";
}

<meta charset="utf-8" />
<script src="~/javascript/jquery-3.4.1.min.js"></script>
<script src="~/javascript/bootstrap.min.js"></script>
<script src="~/javascript/jquery.growl.js" type="text/javascript"></script>
<script src="~/javascript/jquery.dataTables.min.js"></script>
<link rel="stylesheet" href="~/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/TransparenciaEstilo.css" />
<link rel="stylesheet" href="~/css/jquery.growl.css" />
<link rel="stylesheet" href="~/css/jquery.dataTables.min.css" />

<body class="content">
	<div class="container">
		<div class="tab-content">
			<div class="tab-pane active" id="Dashboard">
				<div class="row">
					<h3><span>Últimos Documentos</span></h3>
					<div class="col-12" style="overflow:auto;height:500px">
						<table class="table table-hover" style="margin-top:4%">
							<thead>
								<tr>
									<th style="width:50px" scope="col">Protocolo</th>
									<th style="width:50px" scope="col">Mensagems</th>
									<th style="width:50px" scope="col">Nome do Usuario que cadastrou</th>
									<th style="width:50px" scope="col">Nome do Documento</th>
									<th scope="col"></th>
									<th scope="col"></th>
								</tr>
							</thead>


							@for (int i = 0; i < Model.Count(); i++)
							{
								<tbody>
									<tr class="table-info">
										<td>@Model[i].Id</td>
										<td>@Model[i].Mensagem</td>
										<td>@Model[i].Usuario</td>
										<td>@Model[i].IdDocumento</td>
									</tr>
								</tbody>
							}
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>